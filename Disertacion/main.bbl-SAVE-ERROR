      \strng{namehash}{f4808e0de995258a401c1ed9f7e8e209}
      \strng{fullhash}{5c0e4a815b77a16a93ff6aca2894a2ca}
      \strng{bibnamehash}{f4808e0de995258a401c1ed9f7e8e209}
      \strng{authorbibnamehash}{f4808e0de995258a401c1ed9f7e8e209}
      \strng{authornamehash}{f4808e0de995258a401c1ed9f7e8e209}
